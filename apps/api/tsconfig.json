{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2021", "webworker"],
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"],
    "resolveJsonModule": true,
    "allowJs": false
  },
  "include": ["src"]
}
